    "# Initialize the counter\n",
    "k = 0\n",
    "\n",
    "# Loop using while loop\n",
    "while k < 25:\n",
    "    # Check if the number is even\n",
    "    if k % 2 == 0:\n",
    "        print(k)\n",
    "    # Increment the counter\n",
    "    k += 1"
